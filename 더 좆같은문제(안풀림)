#include <stdio.h>

int main()
{
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);
	int x;
	x = a * b * c;
	int y[30];
	int cnt = x;
	int i = 0;
	int z[10] = { 0 };
	while (cnt > 0) {
		y[i] = cnt % 10;
		cnt = cnt / 10;
		i = i + 1;
	}
	i = 0;
	while (y[i] != NULL) {
		if (y[i] == 0)
			z[0] += 1;
		else if (y[i] == 1)
			z[1] += 1;
		else if (y[i] == 2)
			z[2] += 1;
		else if (y[i] == 3)
			z[3] += 1;
		else if (y[i] == 4)
			z[4] += 1;
		else if (y[i] == 5)
			z[5] += 1;
		else if (y[i] == 6)
			z[6] += 1;
		else if (y[i] == 7)
			z[7] += 1;
		else if (y[i] == 8)
			z[8] += 1;
		else if (y[i] == 9)
			z[9] += 1;

		i += 1;}
	return 0;
}
